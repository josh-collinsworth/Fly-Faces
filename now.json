{
    "version": 2,
    "alias": [
        "fly-faces.now.sh"
    ],
    "env": {
        "CLIENT_ID": "@namely_client_id",
        "CLIENT_SECRET": "@namely_client_secret"
    },
    "github": {
        "autoAlias": true
    },
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        },
        {
            "src": "api/*.js",
            "use": "@now/node"
        }
    ]
}