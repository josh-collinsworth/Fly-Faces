{
    "version": 2,
    "alias": [
        "fly-faces.now.sh"
    ],
    "github": {
        "autoAlias": true
    },
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        },
        {
            "src": "api.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api(.*)",
            "dest": "/api.js"
        }
    ]
}